    <TIER DEFAULT_LOCALE="en" LINGUISTIC_TYPE_REF="Traducción" PARENT_REF="Esteban Castillo García" PARTICIPANT="Esteban Castillo García" TIER_ID="ECG Traducción">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a58" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>El árbol por el que nos están preguntando y sobre el que vamos a platicar es el árbol llamado en mixteco &lt;i&gt;ti¹ya⁴ nda³kwa²&lt;/i&gt;</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a59" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>Ese árbol lo podemos reconocer porque</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a60" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>se parece a los árboles de ciruelos, así son sus troncos con bolas y también se ve que</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a61" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>se les levantan las cáscaras como los árboles del árbol que llamamos &lt;i&gt;tun⁴ ti¹ya⁴ kwa'⁴a¹&lt;/i&gt; o mulato.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a62" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>Así los podemos reconocer y sus hojas son como las hojas de ciruelos.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a63" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>Y no sirven para leña ni otra cosa, creo que no sirven ni para hacer monturas para animales ni nada.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
